{
  "name": "slack-foodee",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">= 8.4"
  },
  "jest": {
    "transform": {},
    "verbose": true,
    "bail": false,
    "testEnvironment": "node",
    "setupFiles": [
      "env-test",
      "should"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/__fixtures.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/fixtures/",
      "/__tests__/__fixtures.js"
    ]
  },
  "description": "Slack slash commands for foodee",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -w index.js -w lib index.js",
    "codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "coverage": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halkeye/slack-foodee.git"
  },
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halkeye/slack-foodee/issues"
  },
  "homepage": "https://github.com/halkeye/slack-foodee#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "axios-debug-log": "^0.4.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "debug": "^3.1.0",
    "errorhandler": "^1.5.0",
    "eslint-plugin-async-await": "0.0.0",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.15.6",
    "grant-express": "^4.1.2",
    "lodash.uniqby": "^4.7.0",
    "memoize": "^0.1.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "raven": "^2.6.4",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "codacy-coverage": "^3.0.0",
    "env-test": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.5.0",
    "nock": "^9.6.1",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1",
    "should": "^13.2.3",
    "supertest": "^3.3.0"
  }
}
