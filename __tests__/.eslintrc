{
  "env": {
    "jest": true
  },
  "extends": ["plugin:jest/recommended", "../"],
  "plugins": ["jest"],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ],
    "func-names": 0,
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0
  }
}
